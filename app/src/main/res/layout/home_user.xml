<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/ll_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


    </LinearLayout>

    <ImageView
        android:id="@+id/iv_logo"
        android:layout_width="@dimen/x250"
        android:layout_height="0dp"
        android:layout_weight="0.4"
        android:layout_marginTop="@dimen/x15"
        android:src="@drawable/logo"
        android:layout_gravity="center"
        />

    <LinearLayout
        android:id="@+id/ll_ItemsParent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x20"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_ac"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_marginBottom="@dimen/x5"
                android:layout_marginRight="@dimen/x5"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/ac" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/ac"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_electrical"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/electrical" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/electrical"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_plumbing"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_marginBottom="@dimen/x5"
                android:layout_marginLeft="@dimen/x5"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/plumbing" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/plumbing"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_furniture"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_marginBottom="@dimen/x5"
                android:layout_marginRight="@dimen/x5"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/furniture"
                    />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/furniture_fixture_assembly"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x12"
                    android:gravity="center_horizontal"
                    app:typeface="Roboto-Regular.ttf"
                    android:layout_marginBottom="@dimen/x5"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_pest"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/pest" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/pest_control"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_cleaning"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_marginBottom="@dimen/x5"
                android:layout_marginLeft="@dimen/x5"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/cleaning" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/cleaning"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_move"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/move" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/move_in_move_out"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_custom"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x110"
                android:layout_marginBottom="@dimen/x5"
                android:layout_marginLeft="@dimen/x5"
                android:layout_weight="1"
                android:background="@color/yellow"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x50"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/x6"
                    android:layout_weight="1"
                    android:src="@drawable/custom" />

                <com.app.yellowcap.ui.views.AnyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.4"
                    android:text="@string/customs_jobs"
                    android:textColor="@color/black"
                    android:textSize="@dimen/x14"
                    app:typeface="Roboto-Regular.ttf" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>